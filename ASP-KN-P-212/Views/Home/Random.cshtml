@model ASP_KN_P_212.Models.Home.Random.RandomPageModel
@{
	ViewData["Title"] = "Random";
}

<div class="text-center">
	<h1 class="display-4">Razor Random Strings</h1>
</div>
<table>
	<thead>
		<tr>
			<th>№</th>
			<th>OTP</th>
			<th>Filename</th>
			<th>Salt</th>
		</tr>
	</thead>
	<tbody>
		@for (int i = 0; i < Model.RandomStrings.Count; i++)
		{
			<tr>
				<td>@(i + 1)</td>
				<td>@Model.RandomStrings[i].Otp</td>
				<td>@Model.RandomStrings[i].Filename</td>
				<td>@Model.RandomStrings[i].Salt</td>
			</tr>
		}
	</tbody>
</table>